{
  "buildCommand": "npm run build:web",
  "outputDirectory": "dist",
  "framework": null,
  "headers": [
    {
      "source": "/(favicon\\.ico|favicon\\.png)",
      "headers": [{ "key": "Cache-Control", "value": "public, max-age=0, must-revalidate" }]
    }
  ],
  "rewrites": [{ "source": "/(.*)", "destination": "/index.html" }]
}
